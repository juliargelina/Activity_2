class CounselingData:
    def __init__(self, date_time, service, session_status, counselor_name, student_name, session_notes, follow_up, next_schedule):  
        
        self.date_time = date_time  
        self.service = service
        self.session_status = session_status
        self.counselor_name = counselor_name
        self.student_name = student_name
        self.session_notes = session_notes
        self.follow_up = follow_up
        self.next_schedule = next_schedule

    def get_date_time(self):  
        return self.date_time
    
    def get_service(self):
        return self.service
    
    def get_session_status(self):
        return self.session_status
    
    def get_counselor_name(self):
        return self.counselor_name
    
    def get_student_name(self):
        return self.student_name
    
    def view_session_notes(self):
        return (self.session_notes)
    
    def get_follow_up(self):
        return (self.follow_up)
    
    def get_next_schedule(self):
        return (self.next_schedule)
 
if __name__ == "__main__":
    CounselingData1 = CounselingData(date_time="July 07 2025, 5:30 - 6:00PM", service="Individual Assessment", session_status="Ongoing", counselor_name="Casy Scout", student_name="Juan A. Dela Cruz", session_notes="Student expressed feelings of burnout because of both school and work schedules.", follow_up="Yes", next_schedule="July 11 2025, 4:00 - 5:00PM")
    
    print("Date and Time:", CounselingData1.date_time)
    print("Type of Service:", CounselingData1.service)
    print("Status", CounselingData1.session_status)
    print("Counselor Name:", CounselingData1.counselor_name)
    print("Student Name:", CounselingData1.student_name)
    print("Session Notes", CounselingData1.session_notes)
    print("Follow Up:", CounselingData1.follow_up)
    print("Next Appointment", CounselingData1.next_schedule)
