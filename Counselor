class Counselor:
 def __init__(self, name, schedule, email):
     self.name = name
     self.schedule = schedule
     self.email = email

 def get_name(self):
      return self.name
 def get_email(self):
      return self.email
 def get_schedule(self):
      return self.schedule
 def update_schedule(self, new_schedule):
     self.schedule = new_schedule
  
counselor_1= Counselor( name = 'Case Scout', schedule = "Mon-Fri 2-5PM", email = "cscout@up.edu.ph")

print("name:", counselor_1.get_name())
print("email:", counselor_1.get_email())
print("schedule:", counselor_1.get_schedule())

counselor_1.update_schedule("New schedule: TTH 1-3PM")
print(counselor_1.get_schedule())
-----------------------------------------------------------------------

NEW VERSION

class Counselor:
 def __init__(self, name, schedule, email):
     self.name = name
     self.schedule = schedule
     self.email = None

 def get_name(self):
      return self.name
 def get_email(self):
      return self.email
 def set_email(self, email):  # FIXED indentation and name
       if '@' in email and '.' in email:
           self.email = email
       else:
           raise ValueError("Invalid email address format")
 def get_schedule(self):
      return self.schedule
 def update_schedule(self, new_schedule):
     self.schedule = new_schedule
  
counselor_1= Counselor( name = 'Case Scout', schedule = "Mon-Fri 2-5PM", email = None)

print("name:", counselor_1.get_name())
print("schedule:", counselor_1.get_schedule())

counselor_1.set_email("cscout@up.edu.ph")
print("email:", counselor_1.get_email())

counselor_1.update_schedule("New schedule: TTH 1-3PM")
print(counselor_1.get_schedule())
